{
  "name": "qre",
  "version": "0.2.6",
  "description": "Quick Regular Expressions - a different approach to the regular expressions syntax.",
  "main": "./dist/cjs/qre.cjs",
  "module": "./dist/esm/qre.mjs",
  "types": "./dist/cjs/qre.d.ts",
  "exports": {
    "types": "./dist/cjs/qre.d.ts",
    "require": "./dist/cjs/qre.cjs",
    "import": "./dist/esm/qre.mjs"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "devDependencies": {
    "@swc/core": "^1.4.1",
    "@types/underscore": "^1.11.15",
    "esbuild": "^0.20.0",
    "fflate": "^0.8.2",
    "showdown": "^2.1.0",
    "showdown-highlight": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "underscore": "^1.13.6",
    "vitest": "^1.3.1",
    "webpack": "^5.90.2",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "prebuild": "npx esbuild --target=chrome62,edge79,firefox53,safari11 --bundle --global-name=GtEa1M6 --sourcemap=linked --minify src/qre.ts --outfile=dist/browser/qre.min.js && npx esbuild --target=chrome62,edge79,firefox53,safari11 --bundle --global-name=GtEa1M6 --sourcemap=linked src/qre.ts --outfile=dist/browser/qre.js",
    "build": "npx tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json",
    "postbuild": "npx ts-node --swc scripts/postbuild.ts",
    "build-web": "npx tsx scripts/build-web/build.ts",
    "test": "npm run test-unit run && npm run test-imports",
    "test-unit": "npx vitest",
    "test-imports": "npx ts-node --swc test/test-imports/run-test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kildom/qre.git"
  },
  "keywords": [
    "regex"
  ],
  "author": "Dominik Kilian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kildom/qre/issues"
  },
  "homepage": "https://kildom.github.io/qre/"
}
